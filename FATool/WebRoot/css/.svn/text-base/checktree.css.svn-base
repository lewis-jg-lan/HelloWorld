
ul.tree, ul.tree * {
	list-style-type: none;
	margin: 0;
	padding: 0 0 5px 0;
}
ul.tree img.arrow {
	padding: 2px 0 0 0;
	border: 0;
	width: 20px;
}
ul.tree li {
	padding: 4px 0 0 0;
}
ul.tree label{
	line-height:25px;
}
ul.tree li ul {
	padding: 0 0 0 20px;
	margin: 0;
}
ul.tree label {
	cursor: pointer;
	font-weight: bold;
	padding: 2px 0;
}
ul.tree label.hover {
	color: red;
}
p {
	margin: 5px 15px;
}
ul {
	margin-top: 5px;
	margin-bottom: 5px;
}
ul.pingpai{width:550px;}
ul.pingpai li{float:left;margin-right:15px;}

ul.tree li .collapsed {
	background-image: url(../img/collapsed.gif);
}
ul.tree li .expanded {
	background-image: url(../img/expanded.gif);
}
ul.tree li .arrow {
	width: 0px;
	height: 25px;
	padding: 0;
	margin: 0;
	cursor: pointer;
	float: left;
	display:none;
	background: transparent no-repeat 0 4px;
}

ul.tree li .checkbox {
	width: 20px;
	height: 18px;
	padding: 0;
	margin: 0;
	cursor: pointer;
	float: left;
	background: url(../img/check0.gif) no-repeat 0 0px;
}
ul.tree li .checked {
	background-image: url(../img/check2.gif);
}
ul.tree li .half_checked {
	background-image: url(../img/check1.gif);
}